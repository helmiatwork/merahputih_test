 <div class="col-2">
    <select id="counter" class="seletize">
        <option value="-">Choose transfer code</option>
        <%@transfer_codes.each do |t|%>
            <option value="<%=t%>"><%=t.to_s.humanize%></option>
        <%end%>
    </select>
    </div>
    <div class="col-3">
    <select id="counter" class="seletize">
        <option value="-">Choose Account</option>
        <%@users.each do |u|%>
            <option value="<%=u.id%>"><%=u.name%></option>
        <%end%>
    </select>
    </div>
</div>